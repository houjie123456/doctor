<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:fitsSystemWindows="true"
    android:clipToPadding="true"
    android:background="#fff">

    <include
        android:id="@+id/head_layout"
        layout="@layout/layout_head_base"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <LinearLayout
        android:id="@+id/line1"
        android:layout_width="0dp"
        android:layout_height="10dp"
        android:background="#f4f4f4"
        android:orientation="horizontal"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/head_layout">
    </LinearLayout>


    <LinearLayout
        android:id="@+id/doc_bind"
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:orientation="horizontal"
        android:background="@drawable/disekuai"
        android:paddingLeft="16dp"
        android:paddingRight="16dp"
        android:gravity="center"
        android:layout_marginLeft="13dp"
        android:layout_marginRight="13dp"
        app:layout_constraintTop_toBottomOf="@+id/line1">
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/icon1">
        </ImageView>
        <com.tencent.qcloud.tuikit.tuichat.fromApp.view.MyTextView
            android:id="@+id/myTextView20"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#333333"
            android:text="医生绑定"
            android:textSize="16sp"
            android:paddingLeft="12dp"
            android:layout_weight="1"/>
        <ImageView
            android:id="@+id/imageView15"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="@+id/myTextView20"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="@+id/myTextView20"
            android:layout_marginRight="8dp"
            android:background="@drawable/img_auth_more"/>
    </LinearLayout>


    <LinearLayout
        android:id="@+id/doc_on_duty"
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:orientation="horizontal"
        android:background="@drawable/disekuai"
        android:paddingLeft="16dp"
        android:paddingRight="16dp"
        android:gravity="center"
        android:layout_marginLeft="13dp"
        android:layout_marginRight="13dp"
        app:layout_constraintTop_toBottomOf="@+id/doc_bind">
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/icon2">
        </ImageView>
        <com.tencent.qcloud.tuikit.tuichat.fromApp.view.MyTextView
            android:id="@+id/myTextView21"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#333333"
            android:text="医生值班"
            android:textSize="16sp"
            android:paddingLeft="12dp"
            android:layout_weight="1"/>
        <ImageView
            android:id="@+id/imageView16"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="@+id/myTextView21"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="@+id/myTextView21"
            android:layout_marginRight="8dp"
            android:background="@drawable/img_auth_more"/>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/pat_management"
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:orientation="horizontal"
        android:background="@drawable/disekuai"
        android:paddingLeft="16dp"
        android:paddingRight="16dp"
        android:gravity="center"
        android:layout_marginLeft="13dp"
        android:layout_marginRight="13dp"
        app:layout_constraintTop_toBottomOf="@+id/doc_on_duty">
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/icon3">
        </ImageView>
        <com.tencent.qcloud.tuikit.tuichat.fromApp.view.MyTextView
            android:id="@+id/myTextView22"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#333333"
            android:text="患者管理"
            android:textSize="16sp"
            android:paddingLeft="12dp"
            android:layout_weight="1"/>
        <ImageView
            android:id="@+id/imageView17"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="@+id/myTextView22"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="@+id/myTextView22"
            android:layout_marginRight="8dp"
            android:background="@drawable/img_auth_more"/>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/on_duty_info"
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:orientation="horizontal"
        android:background="@drawable/disekuai"
        android:paddingLeft="16dp"
        android:paddingRight="16dp"
        android:gravity="center"
        android:layout_marginLeft="13dp"
        android:layout_marginRight="13dp"
        app:layout_constraintTop_toBottomOf="@+id/pat_management">
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/icon4">
        </ImageView>
        <com.tencent.qcloud.tuikit.tuichat.fromApp.view.MyTextView
            android:id="@+id/myTextView23"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#333333"
            android:text="值班情况"
            android:textSize="16sp"
            android:paddingLeft="12dp"
            android:layout_weight="1"/>
        <ImageView
            android:id="@+id/imageView18"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="@+id/myTextView23"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="@+id/myTextView23"
            android:layout_marginRight="8dp"
            android:background="@drawable/img_auth_more"/>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/group_manage"
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:orientation="horizontal"
        android:background="@drawable/disekuai"
        android:paddingLeft="16dp"
        android:paddingRight="16dp"
        android:gravity="center"
        android:layout_marginLeft="13dp"
        android:layout_marginRight="13dp"
        app:layout_constraintTop_toBottomOf="@+id/on_duty_info">
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/icon5">
        </ImageView>
        <com.tencent.qcloud.tuikit.tuichat.fromApp.view.MyTextView
            android:id="@+id/myTextView24"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#333333"
            android:text="群组管理"
            android:textSize="16sp"
            android:paddingLeft="12dp"
            android:layout_weight="1"/>
        <ImageView
            android:id="@+id/imageView19"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="@+id/myTextView24"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="@+id/myTextView24"
            android:layout_marginRight="8dp"
            android:background="@drawable/img_auth_more"/>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
