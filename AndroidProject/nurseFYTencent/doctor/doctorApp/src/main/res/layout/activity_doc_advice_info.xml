<?xml version="1.0" encoding="utf-8"?>

<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:fitsSystemWindows="true"
    android:clipToPadding="true"
    android:background="#fff">
    <include
        android:id="@+id/head_layout"
        layout="@layout/layout_head_base"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>

    <View
        android:id="@+id/line1"
        android:layout_width="0dp"
        android:layout_height="5dp"
        android:background="#f4f4f4"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/head_layout"/>

    <TextView
        android:id="@+id/textView01"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="患者姓名"
        android:textColor="#ff666666"
        android:textSize="13sp"
        android:layout_marginLeft="21dp"
        android:layout_marginTop="15dp"
        app:layout_constraintTop_toBottomOf="@+id/line1"
        app:layout_constraintLeft_toLeftOf="parent"/>
    <TextView
        android:id="@+id/pat_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="徐小小"
        android:textColor="#ff333333"
        android:textSize="13sp"
        android:layout_marginRight="17dp"
        app:layout_constraintTop_toTopOf="@+id/textView01"
        app:layout_constraintRight_toRightOf="parent"/>
    <View
        android:id="@+id/line2"
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:background="#f4f4f4"
        android:layout_marginTop="15dp"
        app:layout_constraintLeft_toLeftOf="@+id/textView01"
        app:layout_constraintRight_toRightOf="@+id/pat_name"
        app:layout_constraintTop_toBottomOf="@+id/textView01"/>
    <TextView
        android:id="@+id/textView02"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="就诊号"
        android:textColor="#ff666666"
        android:textSize="13sp"
        android:layout_marginLeft="21dp"
        android:layout_marginTop="15dp"
        app:layout_constraintTop_toBottomOf="@+id/line2"
        app:layout_constraintLeft_toLeftOf="parent"/>
    <TextView
        android:id="@+id/seek_num"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="12345678"
        android:textColor="#ff333333"
        android:textSize="13sp"
        android:layout_marginRight="17dp"
        app:layout_constraintTop_toTopOf="@+id/textView02"
        app:layout_constraintRight_toRightOf="parent"/>
    <View
        android:id="@+id/line3"
        android:layout_width="0dp"
        android:layout_height="8dp"
        android:background="#f4f4f4"
        android:layout_marginTop="15dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView02"/>


    <TextView
        android:id="@+id/textView03"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="医嘱项目"
        android:textColor="#ff666666"
        android:textSize="13sp"
        android:layout_marginLeft="21dp"
        android:layout_marginTop="15dp"
        app:layout_constraintTop_toBottomOf="@+id/line3"
        app:layout_constraintLeft_toLeftOf="parent"/>
    <TextView
        android:id="@+id/advice_item"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="徐小小"
        android:textColor="#ff333333"
        android:textSize="13sp"
        android:layout_marginRight="17dp"
        app:layout_constraintTop_toTopOf="@+id/textView03"
        app:layout_constraintRight_toRightOf="parent"/>
    <View
        android:id="@+id/line4"
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:background="#f4f4f4"
        android:layout_marginTop="15dp"
        app:layout_constraintLeft_toLeftOf="@+id/textView03"
        app:layout_constraintRight_toRightOf="@+id/pat_name"
        app:layout_constraintTop_toBottomOf="@+id/textView03"/>
    <TextView
        android:id="@+id/textView04"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="标本"
        android:textColor="#ff666666"
        android:textSize="13sp"
        android:layout_marginLeft="21dp"
        android:layout_marginTop="15dp"
        app:layout_constraintTop_toBottomOf="@+id/line4"
        app:layout_constraintLeft_toLeftOf="parent"/>
    <TextView
        android:id="@+id/specimen"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="徐小小"
        android:textColor="#ff333333"
        android:textSize="13sp"
        android:layout_marginRight="17dp"
        app:layout_constraintTop_toTopOf="@+id/textView04"
        app:layout_constraintRight_toRightOf="parent"/>
    <View
        android:id="@+id/line5"
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:background="#f4f4f4"
        android:layout_marginTop="15dp"
        app:layout_constraintLeft_toLeftOf="@+id/textView04"
        app:layout_constraintRight_toRightOf="@+id/pat_name"
        app:layout_constraintTop_toBottomOf="@+id/textView04"/>
    <TextView
        android:id="@+id/textView05"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="医嘱数量"
        android:textColor="#ff666666"
        android:textSize="13sp"
        android:layout_marginLeft="21dp"
        android:layout_marginTop="15dp"
        app:layout_constraintTop_toBottomOf="@+id/line5"
        app:layout_constraintLeft_toLeftOf="parent"/>
    <TextView
        android:id="@+id/advice_num"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="徐小小"
        android:textColor="#ff333333"
        android:textSize="13sp"
        android:layout_marginRight="17dp"
        app:layout_constraintTop_toTopOf="@+id/textView05"
        app:layout_constraintRight_toRightOf="parent"/>
    <View
        android:id="@+id/line6"
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:background="#f4f4f4"
        android:layout_marginTop="15dp"
        app:layout_constraintLeft_toLeftOf="@+id/textView05"
        app:layout_constraintRight_toRightOf="@+id/pat_name"
        app:layout_constraintTop_toBottomOf="@+id/textView05"/>
    <TextView
        android:id="@+id/textView06"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="医嘱备注信息"
        android:textColor="#ff666666"
        android:textSize="13sp"
        android:layout_marginLeft="21dp"
        android:layout_marginTop="15dp"
        app:layout_constraintTop_toBottomOf="@+id/line6"
        app:layout_constraintLeft_toLeftOf="parent"/>
    <TextView
        android:id="@+id/advice_remark"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_marginRight="18dp"
        android:text="未婚、未生育未婚、未生育未婚、未生育未婚、未生育未婚、未生育未婚、未生育未婚、未生育未婚、未生育未婚、未生育未婚、未生育未婚、未生育"
        android:textColor="#4d4d4d"
        android:textSize="13sp"
        app:layout_constraintTop_toBottomOf="@+id/textView06"
        app:layout_constraintLeft_toLeftOf="@+id/textView06"
        app:layout_constraintRight_toRightOf="parent"/>
    <View
        android:id="@+id/line7"
        android:layout_width="0dp"
        android:layout_height="8dp"
        android:background="#f4f4f4"
        android:layout_marginTop="40dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/advice_remark"/>


    <TextView
        android:id="@+id/textView07"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="医嘱执行科室"
        android:textColor="#ff666666"
        android:textSize="13sp"
        android:layout_marginLeft="21dp"
        android:layout_marginTop="15dp"
        app:layout_constraintTop_toBottomOf="@+id/line7"
        app:layout_constraintLeft_toLeftOf="parent"/>
    <TextView
        android:id="@+id/implement_dept"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="徐小小"
        android:textColor="#ff333333"
        android:textSize="13sp"
        android:layout_marginRight="17dp"
        app:layout_constraintTop_toTopOf="@+id/textView07"
        app:layout_constraintRight_toRightOf="parent"/>
    <View
        android:id="@+id/line8"
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:background="#f4f4f4"
        android:layout_marginTop="15dp"
        app:layout_constraintLeft_toLeftOf="@+id/textView07"
        app:layout_constraintRight_toRightOf="@+id/pat_name"
        app:layout_constraintTop_toBottomOf="@+id/textView07"/>
    <TextView
        android:id="@+id/textView08"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="要求执行时间"
        android:textColor="#ff666666"
        android:textSize="13sp"
        android:layout_marginLeft="21dp"
        android:layout_marginTop="15dp"
        app:layout_constraintTop_toBottomOf="@+id/line8"
        app:layout_constraintLeft_toLeftOf="parent"/>
    <TextView
        android:id="@+id/implement_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="12345678"
        android:textColor="#ff333333"
        android:textSize="13sp"
        android:layout_marginRight="17dp"
        app:layout_constraintTop_toTopOf="@+id/textView08"
        app:layout_constraintRight_toRightOf="parent"/>
</androidx.constraintlayout.widget.ConstraintLayout>
