<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="#fff"
    android:fitsSystemWindows="true"
    android:clipToPadding="true">

    <include
        android:id="@+id/head_layout"
        layout="@layout/layout_head_base"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <LinearLayout
        android:id="@+id/line1"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:orientation="horizontal"
        app:layout_constraintTop_toBottomOf="@+id/head_layout"
        android:background="#f4f4f4">
    </LinearLayout>

    <TextView
        android:id="@+id/textView01"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="自动签名数量"
        android:textColor="#ff000000"
        android:textSize="14sp"
        android:layout_marginTop="25dp"
        android:layout_marginLeft="18dp"
        app:layout_constraintTop_toBottomOf="@+id/line1"
        app:layout_constraintLeft_toLeftOf="parent"/>
    <TextView
        android:id="@+id/auto_sign_num"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="69个"
        android:textColor="#ff4d4d4d"
        android:textSize="13sp"
        android:layout_marginRight="18dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView01"/>

    <LinearLayout
        android:id="@+id/line2"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:orientation="horizontal"
        android:background="#f4f4f4"
        android:layout_marginTop="25dp"
        app:layout_constraintTop_toBottomOf="@+id/textView01">
    </LinearLayout>
    <TextView
        android:id="@+id/textView02"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="自动签名结束时间"
        android:textColor="#ff000000"
        android:textSize="14sp"
        android:layout_marginTop="25dp"
        android:layout_marginBottom="25dp"
        android:layout_marginLeft="18dp"
        app:layout_constraintTop_toBottomOf="@+id/line2"
        app:layout_constraintLeft_toLeftOf="parent"/>
    <TextView
        android:id="@+id/auto_sign_end_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="2020-7-15 11:10:11"
        android:textColor="#ff4d4d4d"
        android:textSize="13sp"
        android:layout_marginRight="18dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView02"/>

    <LinearLayout
        android:id="@+id/line3"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:orientation="horizontal"
        android:background="#f4f4f4"
        android:layout_marginTop="25dp"
        app:layout_constraintTop_toBottomOf="@+id/textView02">
    </LinearLayout>
    <TextView
        android:id="@+id/textView03"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="自动签名系统"
        android:textColor="#ff000000"
        android:textSize="14sp"
        android:layout_marginTop="25dp"
        android:layout_marginBottom="25dp"
        android:layout_marginLeft="18dp"
        app:layout_constraintTop_toBottomOf="@+id/line3"
        app:layout_constraintLeft_toLeftOf="parent"/>
    <TextView
        android:id="@+id/auto_sign_system"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="PC客户端"
        android:textColor="#ff4d4d4d"
        android:textSize="13sp"
        android:layout_marginRight="18dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView03"/>

    <LinearLayout
        android:id="@+id/line4"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:orientation="horizontal"
        android:background="#f4f4f4"
        android:layout_marginTop="25dp"
        app:layout_constraintTop_toBottomOf="@+id/textView03">
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>